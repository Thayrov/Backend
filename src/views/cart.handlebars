<div>
  <h1>Cart</h1>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Quantity</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
      <tr>
        <td>{{this.product.title}}</td>
        <td>{{this.quantity}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>

